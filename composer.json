{
    "name": "despark/cms",
    "description": "Despark's Laravel 5.1 project starter.",
    "keywords": ["framework", "laravel", "despark"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "rtconner/laravel-tagging": "~2.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "zizaco/entrust": "^1.4",
        "laravelcollective/html": "~5.0",
        "intervention/image": "^2.3",
        "cviebrock/eloquent-sluggable": "^3.0",
        "despark/html-template-curator": "3.*",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "skovmand/mailchimp-laravel": "1.*",
        "roumen/sitemap": "^2.5",
        "rutorika/sortable": "^2.3",
        "jenssegers/agent": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "corneltek/phprelease": "^1.3"
    },
    "autoload": {
        "classmap": [],
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Despark\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": []
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "beta"
}
